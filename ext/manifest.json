{
  "manifest_version": 2,

  "name": "piOutlets",
  "description": "Chrome extension for piOutlets",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "background": {
    "scripts": ["socketio.js", "background.js"]
  }

}